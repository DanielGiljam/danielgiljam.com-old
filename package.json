{
  "name": "danielgiljam.com",
  "version": "0.0.0",
  "description": "My portfolio site.",
  "homepage": "https://github.com/DanielGiljam/danielgiljam.com#readme",
  "bugs": "https://github.com/DanielGiljam/danielgiljam.com/issues",
  "license": "MIT",
  "private": true,
  "author": "Daniel Giljam (https://github.com/DanielGiljam)",
  "repository": "github:DanielGiljam/danielgiljam.com",
  "scripts": {
    "populate": "ts-node-script -O '{\"module\": \"commonjs\"}' scripts/firestore/populate.ts",
    "dev": "next",
    "build:next": "rimraf .next out && next build && next export",
    "build:functions": "npm --prefix functions run build",
    "build": "npm run build:next && npm run build:functions",
    "emulate": "firebase emulators:start --import saved-data --export-on-exit",
    "build-and-emulate": "npm run build && npm run emulate",
    "tsc": "tsc",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "tff": "npm run tsc && npm run fix && npm run format"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.0.0",
    "firebase-tools": "^8.7.0",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.27.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@danielgiljam/eslint-config": "^2.0.1",
    "@danielgiljam/prettierrc": "^2.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "clsx": "^1.1.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "fontsource-roboto": "^2.2.6",
    "husky": "^4.2.5",
    "next": "^9.4.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark": "^12.0.1",
    "remark-html": "^12.0.0",
    "rimraf": "^3.0.2",
    "svg-inline-loader": "^0.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run lint && npm run prettier"
    }
  }
}
